; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DigitalGate"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Uuid="*res://Uuid.gd"
Classes="*res://Classes.gd"
GateController="*res://GateController.gd"
Gloval="*res://Gloval.gd"
WireController="*res://WireController.gd"
ModeController="*res://ModeController.gd"

[input]

right_click={
"deadzone": 0.5,
"events": []
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
renderer/rendering_method.web="forward_plus"
textures/default_filters/anisotropic_filtering_level=3
anti_aliasing/screen_space_roughness_limiter/limit=0.23
anti_aliasing/quality/msaa_2d=2
anti_aliasing/quality/screen_space_aa=1
